---
import Layout from "@layouts/Layout.astro"
import EntitiesList from "@components/EntitiesList.astro"

interface Props {
  title: string
}

const { title } = Astro.props
---

<Layout title={"Documentation - " + title} isNavBarFixed={false}>
  <main class="flex flex-row my-10">
    <div class="mx-12 max-h-[70vh] px-10 overflow-scroll">
      <h1 class="font-bold underline underline-offset-4">API Contents</h1>
      <EntitiesList />
    </div>
    <slot />
  </main>
</Layout>